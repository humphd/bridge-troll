{
  "name": "bridge-troll",
  "version": "1.0.0",
  "description": "A friendly troll that living under Ontario's bridges",
  "scripts": {
    "generate-bridge-json": "node tools/csv2json.js",
    "build": "webpack --mode production",
    "eslint": "eslint --fix tools src test",
    "prettier": "prettier --single-quote --write \"{tools,src,test}/**/*.{js,css}\"",
    "check-prettier": "prettier --single-quote --list-different \"{tools,src,test}/**/*.{js,css}\"",
    "lint": "npm run eslint && npm run check-prettier",
    "test": "npm run lint",
    "start": "webpack-dev-server --mode development --open",
    "debug": "webpack-dev-server --mode development --define process.env.FAKE_GEO=1 --open"
  },
  "author": "humphd",
  "license": "MIT",
  "devDependencies": {
    "JSONStream": "^1.3.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.10",
    "csv": "^2.0.0",
    "eslint": "^4.18.2",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "prettier": "^1.11.1",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "geo-tree": "^1.0.5",
    "leaflet": "^1.3.1",
    "loglevel": "^1.6.1",
    "suncalc": "^1.8.0"
  }
}
